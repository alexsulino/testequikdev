ch('http://localhost:5000/projects/'+(projectUpdated.id), {\r\n            method: \"PATCH\",\r\n            headers:{\r\n                'Content-type': 'application/json',\r\n            },\r\n            body: JSON.stringify(projectUpdated),\r\n        })\r\n        .then((resp) => resp.json())\r\n        .then((data) => {\r\n          //console.log(data)\r\n          setProject(projectUpdated)\r\n          setServices(servicesUpdated)\r\n          setMessage(\"Serviço removido com sucesso!\")\r\n        })\r\n        .catch((err) => console.log(err))\r\n\r\n\r\n    }\r\n    // End remover  Services\r\n\r\n    function toggleProjectForm() {\r\n        setShowProjectForm(!showProjectForm)\r\n    }\r\n\r\n    function toggleServiceForm() {\r\n        setShowServiceForm(!showServiceForm)\r\n    }\r\n\r\n    return (\r\n        <>\r\n        <div className=\"container Project\">\r\n            <div className=\"container-fluid\">\r\n            \r\n                <div className=\"\">\r\n                <h1 className=\"title\"> Meus Projetos </h1>\r\n\r\n                    <button className=\"btn\" onClick={toggleProjectForm}>\r\n                        {!showProjectForm ?  \" Editar Projeto\" : \"Fechar\"}\r\n                    </button>\r\n                                                   \r\n                    {message && <Message type={type} msg={message}/>}\r\n\r\n                    {!showProjectForm ? (\r\n                        //if showProjectForm\r\n                        <div className={project.id} > \r\n                            {project.name ? <h2 className=\"title\"> {project.name} </h2> : <Loading/>}\r\n                            <p><span>Categoria: </span> {project.category?.name}</p>\r\n                            <p><span>Total Orçamento: </span> R$: {project.budget}</p>\r\n                            <p><span>Total Utilizado: </span> R$: {project.cost}</p>                           \r\n                        </div>\r\n\r\n                    ) : (\r\n                       // else showProjectForm\r\n                        <div className=\"defaultContainer\">\r\n                            <p>  Detalhes do Projeto</p>\r\n\r\n                            <ProjectForm \r\n                            handleSubmit={editPost} \r\n                            btnText=\"Concluir Edição\"\r\n                            projectData={project}/>\r\n                           \r\n                        </div>                     \r\n                    )}\r\n\r\n                    <div className=\"ContainerServicos\">\r\n\r\n                        {/*<h2 className=\"title\"> Adicione um serviço </h2>*/}\r\n                        <button className=\"btn\" onClick={toggleServiceForm}>\r\n                            {!showServiceForm ?  \"Adicionar Serviço\" : \"Fechar\"}\r\n                        </button>            \r\n\r\n                        {showServiceForm && <ServiceForm \r\n                        handleSubmit={createService} \r\n                        btnText=\"Adicionar Serviços\" \r\n                        projectData={project}\r\n                        />}\r\n\r\n                        <h2 className=\"title\">  Serviço</h2>\r\n                        <div className=\"boxSubItens\">\r\n                            {services.length > 0 &&\r\n                                services.map((service) => (\r\n                                    <ServiceCard\r\n                                    id={service.id}\r\n                                    name={service.name}\r\n                                    cost={service.cost}\r\n                                    description={service.description}\r\n                                    key={service.id}\r\n                                    handleRemove={removeService}\r\n                                    />\r\n                                ))\r\n                            \r\n                            }\r\n                            {service